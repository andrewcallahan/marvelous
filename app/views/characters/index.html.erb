<p id="notice"><%= notice %></p>

<h1>Marvel Characters</h1>

<table>
  <tbody>
    <% @characters.first(100).each do |character| %>
      <tr>
        <td><%= link_to image_tag(character.image_url, alt: character.name, width: "100", height: "100"), character %></td>
        <td><%= link_to character.name, character %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>